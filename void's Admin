    function isthatajojoreference()
        attack = true
        BTAUNT2:Stop()
        BTAUNT6:Play()
        for i = 0,7.5,0.1 do
            swait()
            neck.C0 = clerp(neck.C0, necko* CF(0, 0, 0 + ((1* Player_Size) - 1)) * angles(Rad(0 + 0 * Sin(sine / 12)), Rad(0), Rad(30)), 0.5)
